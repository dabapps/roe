{
  "name": "roe",
  "version": "0.0.0",
  "description": "A Collection of React Components for Project Development",
  "main": "src/ts/index.ts",
  "scripts": {
    "start": "./scripts/start",
    "build": "./scripts/build",
    "dist": "./scripts/dist",
    "tests": "jest --no-cache --runInBand",
    "lint": "tslint 'src/ts/**/*.{ts,tsx}' 'docs/src/ts/**/*.{ts,tsx}'",
    "test": "npm run lint && npm run tests",
    "postinstall": "./scripts/dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dabapps/roe.git"
  },
  "keywords": [
    "pdk",
    "project",
    "development",
    "kit",
    "react",
    "components"
  ],
  "author": "DabApps",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/dabapps/roe/issues"
  },
  "homepage": "https://github.com/dabapps/roe#readme",
  "dependencies": {
    "@types/react": "15.0.22",
    "@types/react-dom": "15.5.0",
    "autoprefixer": "6.7.7",
    "browserify": "14.3.0",
    "exorcist": "0.4.0",
    "less": "2.7.2",
    "normalize.css": "6.0.0",
    "postcss": "5.2.17",
    "postcss-cli": "3.2.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "tsify": "3.0.1",
    "typescript": "2.2.2"
  },
  "devDependencies": {
    "@types/jest": "19.2.2",
    "@types/node": "7.0.13",
    "@types/react-test-renderer": "15.4.5",
    "concurrently": "3.4.0",
    "envify": "4.0.0",
    "http-server": "0.9.0",
    "jest": "19.0.2",
    "livereload": "0.6.2",
    "react-test-renderer": "15.5.4",
    "ts-jest": "19.0.10",
    "tslint": "5.1.0",
    "tslint-config-dabapps": "github:dabapps/tslint-config-dabapps#v0.1.0",
    "watch-less-do-more": "0.2.0",
    "watchify": "3.9.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js|jsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "collectCoverage": true
  }
}
